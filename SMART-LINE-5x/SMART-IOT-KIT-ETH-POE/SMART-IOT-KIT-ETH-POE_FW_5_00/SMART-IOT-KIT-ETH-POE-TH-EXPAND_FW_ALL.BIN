								maintenance.percentUnpackComplete++; 
								$("#firmwareBar").width(maintenance.percentUnpackComplete+'%'); 
								$("#percentFirmware").html(maintenance.percentUnpackComplete+'%');
							}, Math.round( ((maintenance.unpackPredictionTime / 100) * 1000)));
						}
					}
			    },
			    success: function()
			    {
					$("#firmwareBar").width('100%');
					$("#percentFirmware").html('100%');
					$("#messageFirmware").html("<br><center>Upload firmware finished.<br>Installing new firmware.</center>");
					
			    },
				complete: function(response)
				{
					clearTimeout(maintenance.unpackRefresh);
					maintenance.unpackRefresh = false;
					maintenance.percentUnpackComplete = 100;
					$("#firmwareBar").width(maintenance.percentUnpackComplete+'%'); 
					$("#percentFirmware").html(maintenance.percentUnpackComplete+'%');
					var resp = response.responseJSON[0];
					if (resp.status == "OK") {
						$("#messageFirmware").html("<br><center><font color='green'><center>Uploading of the file finished.<br>Wait 60 seconds to restart device and reload page.</center></font></center>");
					} else {
						if (resp.message == "1") {
							$("#messageFirmware").html("<br><center><font color='red'>Unable to properly upload the file.</font></center>");
						}
					}
				},
				error: function(error)
				{
					$("#messageFirmware").html("<font color='red'> ERROR: unable to upload files</font>");
				}
			};
			$('#formfirmware').ajaxForm(options);
		},
		
		uploadConfig:function(){
			var options = {
			    beforeSubmit: function()
			    {
					maintenance.fsize = $('#config_file')[0].files[0].size;
					maintenance.unpackPredictionTime = maintenance.fsize / maintenance.speed;
					maintenance.percentUnpackComplete = 0;
			    },
			    beforeSend: function()
			    {
					$('#modalWindowUpgradeFirmware').modal();
					$("#firmwareProgress").show();
					$("#firmwareBar").width('0%');
					$("#messageFirmware").html("");
					$("#percentFirmware").html("0%");
					$("#messageFirmware").html("<br><center>Uploading config...</center>");
			    },
			    uploadProgress: function(event, position, total, percentComplete)
			    {
					$("#firmwareBar").width(percentComplete+'%');
					$("#percentFirmware").html(percentComplete+'%');
					if (percentComplete == 100) {
						$("#messageFirmware").html("<br><center>Unpacking new configuration...</center>");
						if ( maintenance.unpackRefresh == false ) {
							maintenance.unpackRefresh = setInterval(function(){ 
								maintenance.percentUnpackComplete++; 
								$("#firmwareBar").width(maintenance.percentUnpackComplete+'%'); 
								$("#percentFirmware").html(maintenance.percentUnpackComplete+'%');
							}, Math.round( ((maintenance.unpackPredictionTime / 100) * 1000)));
						}
					}
			    },
			    success: function()
			    {
					$("#firmwareBar").width('100%');
					$("#percentFirmware").html('100%');
					$("#messageFirmware").html("<br><center>Upload config finished.<br>Unpacking new configuration.</center>");
					
			    },
				complete: function(response)
				{
					clearTimeout(maintenance.unpackRefresh);
					maintenance.unpackRefresh = false;
					maintenance.percentUnpackComplete = 100;
					$("#firmwareBar").width(maintenance.percentUnpackComplete+'%'); 
					$("#percentFirmware").html(maintenance.percentUnpackComplete+'%');
					var resp = response.responseJSON[0];
					if (resp.status == "OK") {
						$("#messageFirmware").html("<br><center><font color='green'><center>Uploading of the file finished.<br>Wait 60 seconds to restart device and reload page.</center></font></center>");
					} else {
						if (resp.message == "1") {
							$("#messageFirmware").html("<br><center><font color='red'>Unable to properly upload the file.</font></center>");
						}
						if (resp.message == "2") {
							$("#messageFirmware").html("<br><center><font color='red'>Error in extracting firmware.</font></center>");
						}
						if (resp.message == "3") {
							$("#messageFirmware").html("<br><center><font color='red'>Checksum error.Please try again.</font></center>");
						}
					}
				},
				error: function(error)
				{
					$("#messageFirmware").html("<font color='red'> ERROR: unable to upload files</font>");
				}
			};
			$('#formConfig').ajaxForm(options);
		}
};
