		convertSensorData : function (_idx) {
				var _date = new Date();
				var _t = [];
				var _tmp_data = [];
				
				for ( var j = 0; j < _last24h_history_data.length; j++ ) {
					var data = _last24h_history_data[j];
					var _d = data.split("\n");
					for ( var i = 1; i < _d.length - 1; i++ ) {
						var _v = [];
						if ( _d[i].indexOf(";") != -1 ) _v = _d[i].split(";");
						if ( _d[i].indexOf(",") != -1 ) _v = _d[i].split(",");
						if ( _d[i].indexOf("|") != -1 ) _v = _d[i].split("|");
						_tmp_data.push (_v);
					}
					
					if ( _tmp_data.length > 0 )
						_tmp_data.sort(sortFunctionFirstColumn);
				}
				
				for ( var j = 0; j < _tmp_data.length; j++ ) {
						var _data = _tmp_data[j];
						var _val = -3000.0;
						if (typeof _data[_idx+1] === "undefined") {
							_val = -3000.0;
						} 
						if (typeof _data[_idx+1] === "string") {
							if (_data[_idx+1].trim() == "") {
								_val = -3000.0;
							} else {
								_val = _data[_idx+1]; 
							}
						}
						
						var _y = [];
						if ( parseFloat(_val) == -3000.0 ) {
							_t.push( "null" );
						} else {
							_data[0] = _data[0].replace(/\((.+?)\)/g, '')
							_data[0] = _data[0].replace(' ','T');
							_data[0] = _data[0] + 'Z';
							var _x = new Date(_data[0]).getTime() + _date.getTimezoneOffset()*60*1000;
							var _year = new Date(_data[0]).getFullYear();
							if ( _year != 1970 ) {
								// tylko ostatnie 24 godziny
								if ( ( _date.getTime() - _x ) < (86400*1000) ) {
									_y.push ( _x.toString() );
									_y.push ( _val );
									_t.push ( _y );
								}
							}
						}
				}
				
				return _t;
		},
		
		calcAbsoluteHumidity: function ( _temp, _hum ) {
			var temp = parseFloat (_temp);
			var hum  = parseFloat (_hum);
			var ai = 7.45;
			var bi = 235;    
			var temp_1 = (ai*temp)/(bi+temp); 
			var temp_2 = 6.1*Math.exp(temp_1*2.3025851);
			var temp_3 = temp_2*hum/100;
			var _return1 = 216.7*temp_3;
			var _return2 = 273.15 + temp;
			return ((_return1)/(_return2));
		},
		
		calcDevPoint: function ( _temp, _hum ) {
			var temp = parseFloat (_temp);
			var hum  = parseFloat (_hum);
			var temp_1 = hum / 100.0;
			var temp_2 = 112.0 + ( 0.9 * temp );
			var temp_3 = 0.1 * temp;
			temp_1 = Math.sqrt(temp_1);
			temp_1 = Math.sqrt(temp_1);
			temp_1 = Math.sqrt(temp_1);    
			return (temp_1 * temp_2 + temp_3 - 112.0);
		},
		
		calculateDevPointsData : function ( _temp_data, _hum_data) {
			var dev_point_data = [];
			
			for ( var j = 0; j < _temp_data.length; j++ ) {
				var _entry_temp = _temp_data[j];
				var _entry_hum = _hum_data[j];
				
				if ((typeof _entry_temp === "string") || (typeof _entry_hum === "string") ) {
					dev_point_data.push( "null" );
				} else {
					var _date = _entry_temp[0];
					var temp = _entry_temp[1];
					var hum = _entry_hum[1];
					if ( (temp != -3000.0) && (hum != -3000.0) ) {
						var _val = motes.calcDevPoint (temp,hum);
						var _entry = [];
						_entry.push ( _date.toString() );
						_entry.push ( _val );
						dev_point_data.push ( _entry ); 
					} else {
						dev_point_data.push( "null" );
					}
				}
			}
			
			return dev_point_data;
			
		},
		calculateAbsoluteHumidityPointsData : function ( _temp_data, _hum_data) {
			var abs_hum_data = [];
			
			for ( var j = 0; j < _temp_data.length; j++ ) {
				var _entry_temp = _temp_data[j];
