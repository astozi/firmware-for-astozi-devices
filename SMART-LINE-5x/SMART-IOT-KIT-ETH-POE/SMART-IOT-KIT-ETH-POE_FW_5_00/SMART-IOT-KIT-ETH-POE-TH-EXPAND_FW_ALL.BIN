var system_functions = {

		timeConverter : function(UNIX_timestamp, type){
				if (type === undefined) type = "datetime";
				var formattedTime = "";
				var x = UNIX_timestamp*1000;
				var date = new Date(x);
				var year = date.getFullYear();
				var month = ((date.getMonth() + 1) < 10 ? '0' + (date.getMonth() + 1) : (date.getMonth() + 1));
				var day = (date.getDate() < 10 ? '0' + date.getDate() : date.getDate());
				var hours = (date.getHours() < 10 ? '0' + date.getHours() : date.getHours());
				var minutes = minutes = (date.getMinutes() < 10 ? '0' + date.getMinutes() : date.getMinutes());
				var seconds = (date.getSeconds() < 10 ? '0' + date.getSeconds() : date.getSeconds());

				if (type == "datetime")
					formattedTime = year + '-' + month + '-' + day + ' ' + hours + ':' + minutes + ':' + seconds;
				else if (type == "date")
					formattedTime = year + '-' + month + '-' + day;	
				else if (type == "time")
					formattedTime = hours + ':' + minutes + ':' + seconds;

				return formattedTime;
		},
		
		genUUID : function() {
			function s4() {
				return Math.floor((1 + Math.random()) * 0x10000).toString(16).substring(1);
			}
			return s4() + s4() + '-' + s4() + '-' + s4() + '-' + s4() + '-' + s4() + s4() + s4();
		},
		
		ColorLuminance: function(hex, lum) {
			hex = String(hex).replace(/[^0-9a-f]/gi, '');
			if (hex.length < 6) {
				hex = hex[0]+hex[0]+hex[1]+hex[1]+hex[2]+hex[2];
			}
			lum = lum || 0;
			var rgb = "#", c, i;
			for (i = 0; i < 3; i++) {
				c = parseInt(hex.substr(i*2,2), 16);
				c = Math.round(Math.min(Math.max(0, c + (c * lum)), 255)).toString(16);
				rgb += ("00"+c).substr(c.length);
			}
			return rgb;
		},
		
