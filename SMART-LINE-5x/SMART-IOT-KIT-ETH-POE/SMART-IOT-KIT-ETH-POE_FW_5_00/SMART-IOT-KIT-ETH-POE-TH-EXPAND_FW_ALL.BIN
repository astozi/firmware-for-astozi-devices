							'<td data-title="Enable"><input id="coap_publish_sensors-' + i + '-switch-enable" name="coap_publish_sensors-' + i + '-switch-enable" type="checkbox" checked></td>' +
							'</tr>';
							$("#coap_publish_sensors_data").append(line);
						if (data[0].ext_m_d_p_co_p_e == "1")	$('#coap_publish_sensors-' + i + '-switch-enable').attr('checked', true);
						else 									$('#coap_publish_sensors-' + i + '-switch-enable').attr('checked', false);
						i = i + 1;
						line = "<tr>" +
							'<td data-title="Id" style="display: none">' + i + '</td>' +
							'<td data-topic="' + 'absolute-humidity' + '" data-title="Topic">' + 'sensor/absolute-humidity' + '</td>' +
							'<td data-title="Value">' + data[0].ext_m_a_h + '</td>' +
							'<td data-title="Enable"><input id="coap_publish_sensors-' + i + '-switch-enable" name="coap_publish_sensors-' + i + '-switch-enable" type="checkbox" checked></td>' +
							'</tr>';
							$("#coap_publish_sensors_data").append(line);
						if (data[0].ext_m_a_h_co_p_e == "1")	$('#coap_publish_sensors-' + i + '-switch-enable').attr('checked', true);
						else 									$('#coap_publish_sensors-' + i + '-switch-enable').attr('checked', false);
						i = i + 1;
					}
					
					
				}).fail(function( jqXHR, textStatus ) {
			    	//requestErrorHandler(jqXHR, textStatus);
			    }).always(function( data ) {
			    	//console.log(data);
				});
		},
		
		schedule : function(){
			setInterval(function(){ coap.getParams(); }, 30000);
		},

		eventsOnClick : function(){
			$("#coap-button-save").on('click',coap.coapParamsSet);
			$("#coap-publish-parameters-button-set").on('click',coap.coapPublishParamsSet);
		},

		coapParamsSet : function(){
			var json_query =  { 
				"reboot": "true",
				"co_e": $('#group-switch-coap-enable').bootstrapSwitch('state').toString(),
				"co_a_e": $('#group-switch-coap-restrict-enable').bootstrapSwitch('state').toString(),
				"co_a_h": $('#coap_access_host').val().toString()
			};
			$.ajax({
				url: apiUrl + 'CONFIG.SET',
				data: JSON.stringify(json_query),
				beforeSend: function(xhr) {
					$.spin(true);
				}
			}).done(function ( data ) {
				coap.getParams();
				if (data[0].status == "OK"){
					$('#modalWindowMessage').modal();
					$('#modalWindowMessage .modal-title').html("OK");
					$('#modalWindowMessage .modal-body').html('<CENTER>Saving config and restart mote. <BR>Wait 60 seconds and refresh website.</CENTER>');
				} else {
					$('#modalWindowMessage').modal();
					$('#modalWindowMessage .modal-title').html("FAILED");
					$('#modalWindowMessage .modal-body').html('<CENTER>' + data[0].message + '</CENTER>');
				}
			}).success(function ( data ) {
				//console.log(data);
			}).fail(function( jqXHR, textStatus ) {
				//console.log("fail");
			}).always(function() {
				$.spin(false);
			});
		},
		
		coapPublishParamsSet:function( ){
			
			var _topic_name = '';
			var _topic_pub_enable = '';
			
			$('#coap_publish_indicators_data > tr').each(function( index ) {
				var t = $(this);
				t.children().each(function ( index ) {
					if ( index == 1 ) {
					  _topic_name =  $(this).html();
					}
					if ( index == 3 ) {
					  _topic_pub_enable = ( $(this).find("input").is(':checked') == true ? "1" : "0" )
					}
				});

				var json_query =  { 
					"reboot": "false",
					"t_n": _topic_name.toString(),
					"co_p_e": _topic_pub_enable.toString()
				};
				$.ajaxq('Queue',{
					url: apiUrl + 'CONFIG.SET',
					data: JSON.stringify(json_query),
					beforeSend: function(xhr) {
						$.spin(true);
					}
				}).done(function ( data ) {
					//console.log(data);
					//sleep(1000);
				}).success(function ( data ) {
					//console.log(data);
				}).fail(function( jqXHR, textStatus ) {
					//console.log("fail");
					status = 0;
				}).always(function() {
					$.spin(false);
				});
			});
			
			$('#coap_publish_sensors_data > tr').each(function( index ) {
				var t = $(this);
				t.children().each(function ( index ) {
					if ( index == 1 ) {
					  //_topic_name =  $(this).html();
					  _topic_name =  $(this).data('topic');
					}
					if ( index == 3 ) {
					  _topic_pub_enable = ( $(this).find("input").is(':checked') == true ? "1" : "0" )
					}
				});

				var json_query =  { 
					"reboot": "false",
					"t_n": _topic_name.toString(),
					"co_p_e": _topic_pub_enable.toString()
				};
				$.ajaxq('Queue',{
					url: apiUrl + 'CONFIG.SET',
					data: JSON.stringify(json_query),
					beforeSend: function(xhr) {
						$.spin(true);
					}
				}).done(function ( data ) {
					//console.log(data);
					//sleep(1000);
				}).success(function ( data ) {
					//console.log(data);
				}).fail(function( jqXHR, textStatus ) {
					//console.log("fail");
					status = 0;
				}).always(function() {
					$.spin(false);
				});
			});

			system_functions.save_config_quiet(coap.getParams);
		}

	};
