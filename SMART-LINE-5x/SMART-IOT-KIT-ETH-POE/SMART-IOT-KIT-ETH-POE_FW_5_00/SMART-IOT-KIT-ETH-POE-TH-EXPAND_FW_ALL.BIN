		convertUpTimetoHHMMSS : function (_uptime) {
			var sec_num = parseInt(_uptime, 10); // don't forget the second param
			var sec_num = sec_num / 100;
			var days    = Math.floor(sec_num / 86400);
			var hours   = Math.floor((sec_num - (days * 86400)) / 3600);
			var minutes = Math.floor((sec_num - (days * 86400) - (hours * 3600)) / 60);
			var uptime_string    = days+" days, "+hours+' hours, '+minutes+' minutes';
			return uptime_string;
		},
		
