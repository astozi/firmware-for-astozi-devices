		drawGraph : function (divId, data, graphType, summary) {
					
					if (typeof summary === "undefined") summary = false;
					
					var stack = 0, bars = false, lines = true, steps = false;
					function getCol(matrix, col){ var column = []; for(var i=0; i<matrix.length; i++){ column.push(matrix[i][col]); } return column; }
					function removeArrayItem (arr, item) { var removeCounter = 0; for (var index = 0; index < arr.length; index++) { if (arr[index] === item) { arr.splice(index, 1); removeCounter++; index--; } } return removeCounter; }
					
					function dataFormatter(v, axis) {
						return v.toFixed(axis.tickDecimals) +"";
					}

					function plotWithOptions(chartId,graphType) {
						if ( summary == false ) {
							main_chart = fplot ($("#" + chartId),
								_data,
							{
								grid: {
									borderWidth: 0, hoverable: true, color: "#777"
								},
								xaxes: [ { mode: "time", timezone: "browser" } ],
								yaxes: _yaxis,
								colors: [ sensors_graph_colors[graphType] ],
								legend: { show: false }
							});
							motes.data2export2csv = system_functions.getDataFromGraph( main_chart );
						}
						
						if ( summary == true ) {
						
							main_chart = fplot ($("#" + chartId),
								_data,
							{
								grid: {
									borderWidth: 0, hoverable: true, color: "#777"
								},
								xaxes: [ { mode: "time", timezone: "browser" } ],
								yaxes: _yaxis,
								colors: [ sensors_graph_colors[graphType] ],
								selection: { mode: "xy" }
							});
							
							motes.data2export2csv = system_functions.getDataFromGraph( main_chart );
							
							summary_chart  = fplot ($("#summary-" + chartId),
								_data,
							{
								series: {
									lines: {
										show: true,
										lineWidth: 1
									},
									shadowSize: 0
								},
								xaxes: [ { mode: "time", timezone: "browser" } ],
								yaxes: _yaxis,
								colors: [ sensors_graph_colors[graphType] ],
								legend: { show: false },
								selection: { mode: "xy" }
							});
							$("#" + chartId).bind("plotselected", function (event, ranges) {
								$.each(main_chart.getXAxes(), function(_, axis) {
									var opts = axis.options;
									opts.min = ranges.xaxis.from;
									opts.max = ranges.xaxis.to;
								});
								$.each(main_chart.getYAxes(), function(_, axis) {
									var opts = axis.options;
									opts.min = ranges.yaxis.from;
									opts.max = ranges.yaxis.to;
								});
								motes.data2export2csv = system_functions.getDataFromGraph( main_chart, ranges.xaxis.from, ranges.xaxis.to );
								main_chart.setupGrid();
								main_chart.draw();
								main_chart.clearSelection();
								summary_chart.setSelection(ranges, true);
							});
							$("#summary-" + chartId).bind("plotselected", function (event, ranges) {
								main_chart.setSelection(ranges);
								main_chart.setupGrid();
								main_chart.draw();
								main_chart.clearSelection();
							});
						}
					}
					
				   function showTooltip(x, y, contents) {
						$('<div id="tooltip">' + contents + '</div>').css( {
							position: 'absolute',
							display: 'none',
							top: y + 5,
							left: x + 5,
							border: '1px solid #fdd',
							padding: '2px',
							'background-color': '#fee',
							'z-index': 2000,
							opacity: 0.80
						}).appendTo("body").fadeIn(200);
						
						var windows_size = $( window ).width();
						var tooltip_size = $( '#tooltip ').width();
						if (tooltip_size + x + 5 > windows_size) {
							$( '#tooltip ').css(  {
								left: x + 5 - tooltip_size
							});
						}
					}
					
					if ( typeof data === "undefined" ) {
						$('#'+divId).html("<br><br><br><center>Waiting for data...</center>");
					} else {
						var avg = getCol (data,1);
						var avg2 = avg;
						removeArrayItem(avg, "u");
						
						var min_val = (Math.min.apply(Math,avg2));
						var max_val = (Math.max.apply(Math,avg2));
						var distance_val = max_val - min_val;
						var _label1 = "";
						var _unit1 = "";
						if ( motes.mote.ext_m == "1") {
							if ( graphType == "Temperature" ) {
								_label1 = "Temperature";
								_unit1 = sensors_units_array[graphType];
							}
							if ( graphType == "Humidity" ) {
								_label1 = "Relative humidity"
								_unit1 = sensors_units_array[graphType];
							}
						}
						
						var _data = [ { data: data, label: _label1, yaxis: 1, unit: _unit1 } ];
						var _yaxis = [ { position: "left",  tickFormatter: dataFormatter, min: min_val-0.5*distance_val, max: max_val+0.5*distance_val } ];
						
						if ( (motes.mote.ext_m == "1") && (motes.device_type != "256")) {
							var _second_yaxis = { position: "right",  tickFormatter: dataFormatter };
							_yaxis.push (_second_yaxis);
							var _label2 = "";
							var _data2 = [];
							var _unit2 = "";
							if ( graphType == "Temperature" ) {
								_label2 = "Dev point";
								_data2 = motes.dev_point_data;
								_unit2 = sensors_units_array[graphType];
							}
							if ( graphType == "Humidity" ) {
								_label2 = "Absolute humidity";
								_data2 = motes.abs_hum_data;
								_unit2 = "g/m3";
							}
							var _second_data = { data: _data2, label: _label2, yaxis: 2, unit: _unit2  };
							_data.push (_second_data);
						}
						
						if ( avg.length == 0 ) {
							$('#'+divId).html("<br><br><br><center>Waiting for data...</center>");
						} else {
							plotWithOptions(divId, graphType);
						}
						
						var previousPoint = null;
						$("#" + divId).bind("plothover", function (event, pos, item) {
							$("#x").text(pos.x.toFixed(2));
							$("#y").text(pos.y.toFixed(2));
							if (item) {
								if (previousPoint != item.dataIndex) {
									previousPoint = item.dataIndex;
									$("#tooltip").remove();
									var _label = item.series.label;
									var _unit = item.series.unit;
									var x = item.datapoint[0],
										y = item.datapoint[1].toFixed(2);
									var date = new Date(x);
